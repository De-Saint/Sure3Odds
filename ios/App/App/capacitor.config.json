{
  "appId": "com.sure3odds.sureapp",
  "appName": "Sure3Odds",
  "bundledWebRuntime": false,
  "npmClient": "npm",
  "webDir": "www",
  "android": {
    "allowMixedContent": true
  },
  "server": {
    "androidScheme": "http",
    "cleartext": true
  },
  "plugins": {
    "SplashScreen": {
      "launchShowDuration": 3000,
      "launchAutoHide": false,
      "androidScaleType": "CENTER_CROP",
      "FadeSplashScreen": false,
      "androidSplashResourceName": "splash"
    }
  },
  "cordova": {}
}
